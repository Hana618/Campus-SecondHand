/* 启动页样式 */
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  height: 100vh;
  background-color: #ffffff;
  position: relative;
  overflow: hidden;
  padding-top: 80px;
}

/* 装饰性圆形元素 */
.container::before,
.container::after {
  content: '';
  position: absolute;
  border-radius: 50%;
  background-color: #88ff88;
  opacity: 0.5;
  z-index: 0;
}

.container::before {
  top: -50px;
  right: -50px;
  width: 200px;
  height: 200px;
}

.container::after {
  bottom: -50px;
  left: -50px;
  width: 150px;
  height: 150px;
}

/* 头部样式 */
.header {
  width: 100%;
  text-align: center;
  padding: 20px 0;
  font-size: 20px;
  color: #333333;
  font-weight: bold;
  z-index: 1;
}

/* 内容区域 */
.content {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  z-index: 1;
  margin: 20px 0;
}

.replaceimg {
  width: 200px;
  height: 200px;
  margin-bottom: 20px;
  border-radius: 10px;
}

.version {
  font-size: 18px;
  color: #333333;
  font-weight: bold;
}

/* 底部样式 */
.footer {
  position: absolute;
  bottom: 40px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  z-index: 1;
}

.footer view {
  margin-bottom: 5px;
}

.footer text {
  font-size: 14px;
  color: #666666;
  text-align: center;
}
